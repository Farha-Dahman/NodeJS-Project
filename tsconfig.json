{
    "version": "5.1.6",
    "compilerOptions": {
        "lib": ["es5", "es6"],
        "target": "es6",
        "module": "CommonJS",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": false,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
    },
    "exclude": [
        "node_modules"
    ]
}
