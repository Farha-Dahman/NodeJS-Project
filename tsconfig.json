{
  "compilerOptions": {
    "lib": ["es5", "es6"],
    "target": "es6",
    "module": "CommonJS",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "strictPropertyInitialization": false,
    "strict": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules"]
}
